<mvc:View
    controllerName="fr.applium.afl.aflapproval.controller.App"
    displayBlock="true"
    height="100%"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:mvc="sap.ui.core.mvc">
    <App
        id="app"
        busy="{appView>/busy}"
        busyIndicatorDelay="0">
        <f:FlexibleColumnLayout
            id="layout"
            layout="{appView>/layout}"
            backgroundDesign="Translucent">
        </f:FlexibleColumnLayout>
    </App>
</mvc:View>