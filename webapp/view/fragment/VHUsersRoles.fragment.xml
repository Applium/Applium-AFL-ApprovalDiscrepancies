<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:core="sap.ui.core"
	xmlns:filterbar="sap.ui.comp.filterbar">

	<ValueHelpDialog
		title="{/#VHUsersRoles/UserId/@sap:label}"
		busy="{appView>/busyVH}"
		ok=".onOkVHUsersRoles"
		cancel=".onCancelVHUsersRoles"
		afterClose="onAfterCloseVHUsersRoles"
		supportMultiselect="true"
		key="UserId"
		descriptionKey="UserFullName" >
		<filterBar>
			<filterbar:FilterBar
				search=".onSearchVHUsersRoles"
				showFilterConfiguration="false"
				showGoOnFB="false" >
			</filterbar:FilterBar>
		</filterBar>

	</ValueHelpDialog>
</core:FragmentDefinition>